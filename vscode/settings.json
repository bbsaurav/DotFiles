{
  "[go]": {
    "editor.tabSize": 8,
    "editor.insertSpaces": false,
    "editor.detectIndentation": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true,
        "source.fixAll": true
    }
  },
  "editor.rulers": [
    80
  ],
  "java.completion.importOrder": [
    "java",
    "javax",
    "com",
    "org"
  ]
}